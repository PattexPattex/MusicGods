<?xml version="1.0" encoding="UTF-8"?>
<configuration packagindData="true">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{dd-MM-yy HH:mm:ss.SSS}] [%thread] [%logger{36}] [%-4level] - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>